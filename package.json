{
  "name": "ts-auto-mock-env",
  "version": "1.0.0",
  "author": "martinjlowm <martin@martinjlowm.dk>",
  "license": "MIT",
  "bin": {
    "generate-mocks": "./scripts/generate-mocks.js"
  },
  "scripts": {
    "test": "yarn generate-mocks test/mocks/tsconfig.json && grep 'globalFunction' test/mocks/index.js >/dev/null",
    "dist:build": "tsc --outDir dist",
    "dist:collect": "cp package.json dist"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "eslint": "^7.3.0",
    "husky": "^4.2.5",
    "ts-auto-mock": "^2.2.8",
    "ts-node": "^8.10.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5",
    "typescript-language-server": "^0.4.0"
  },
  "peerDependencies": {
    "ts-auto-mock": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc --noEmit",
      "pre-push": "yarn version check"
    }
  }
}
